  </servers>

  <!-- To defined a repo to use -->
  <profiles>
    <profile>
      <id>test</id>
      <repositories>
        <repository>
          <id>jetty-local</id>
          <name>Local Jetty Repository</name>
          <url>http://localhost:8778</url>
        </repository>
      </repositories>
    </profile>
  </profiles>

  <activeProfiles>
    <activeProfile>test</activeProfile>
  </activeProfiles>
  
